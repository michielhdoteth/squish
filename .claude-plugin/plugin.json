{
  "name": "squish-memory",
  "version": "0.3.0",
  "description": "Local-first persistent memory for Claude Code with lifecycle management, agent isolation, memory governance, and team sync",
  "author": {
    "name": "michielhdoteth"
  },
  "license": "MIT",
  "homepage": "https://github.com/michielhdoteth/squish",
  "repository": "https://github.com/michielhdoteth/squish.git",
  "keywords": [
    "claude",
    "claude-code",
    "plugin",
    "memory",
    "persistent-memory",
    "semantic-search",
    "full-text-search",
    "local-first",
    "privacy"
  ],
  "commands": [
    {
      "name": "remember",
      "description": "Store a memory with semantic embeddings"
    },
    {
      "name": "recall",
      "description": "Retrieve a memory by ID"
    },
    {
      "name": "search",
      "description": "Full-text and semantic search across memories"
    },
    {
      "name": "conversations",
      "description": "Search past conversations"
    },
    {
      "name": "observe",
      "description": "Store observations from tool usage"
    },
    {
      "name": "context",
      "description": "Get project context with relevant memories"
    },
    {
      "name": "health",
      "description": "Check service status"
    },
    {
      "name": "forget",
      "description": "Delete a memory by ID"
    },
    {
      "name": "snapshots",
      "description": "List memory snapshots"
    },
    {
      "name": "requirements",
      "description": "Track project requirements"
    }
  ]
}
